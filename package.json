{
  "name": "psy-scale-angular",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --port 22222",
    "start:mock": "ng serve --configuration mock --port 22222",
    "start:prod": "ng serve --configuration production --port 22222",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "test:coverage": "ng test --code-coverage --watch=false"
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "overrides": [
      {
        "files": "*.html",
        "options": {
          "parser": "angular"
        }
      }
    ]
  },
  "private": true,
  "dependencies": {
    "@angular/common": "^20.3.7",
    "@angular/compiler": "^20.3.7",
    "@angular/core": "^20.3.7",
    "@angular/forms": "^20.3.7",
    "@angular/platform-browser": "^20.3.7",
    "@angular/router": "^20.3.7",
    "@ngxs/devtools-plugin": "20.1.0",
    "@ngxs/form-plugin": "20.1.0",
    "@ngxs/hmr-plugin": "20.1.0",
    "@ngxs/logger-plugin": "20.1.0",
    "@ngxs/router-plugin": "20.1.0",
    "@ngxs/storage-plugin": "20.1.0",
    "@ngxs/store": "^20.1.0",
    "@ngxs/websocket-plugin": "20.1.0",
    "ng-zorro-antd": "^20.3.1",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^20.3.7",
    "@angular/build": "^20.3.7",
    "@angular/cli": "^20.3.7",
    "@angular/compiler-cli": "^20.3.7",
    "@types/jasmine": "~5.1.0",
    "jasmine-core": "~5.9.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "typescript": "~5.9.2"
  }
}
